INSERT INTO todo_list VALUES (3, "Practice SQL", 30);
INSERT INTO todo_list VALUES (4, 'Read', 30);
INSERT INTO todo_list VALUES (5, "Work", 50);
INSERT INTO todo_list VALUES (6, "Shopping", 20); -- Values inserted for the to do list

SELECT SUM(minutes)
FROM todo_list; -- Total of minutes to finish the to do list


